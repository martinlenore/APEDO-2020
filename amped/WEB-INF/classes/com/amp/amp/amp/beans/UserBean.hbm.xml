<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.amp.amp.amp.beans.UserBean" table="user_tbl">
         <id name="id" column="id" type="java.lang.Integer">
           <generator class="native"/>
         </id>
		   <property name="Username" type="java.lang.String" column="UserName"/> 
		   <property name="password" type="java.lang.String" column="password"/> 
		   <property name="accesslevel" type="java.lang.String" column="accesslevel"/>
		   <property name="firstname" type="java.lang.String" column="First_Name"/> 
		   <property name="lastname" type="java.lang.String" column="Last_Name"/> 
           <property name="email" type="java.lang.String" column="email"/> 
          <property name="contactno" type="java.lang.String" column="Contact_No"/> 
		  <property name="jobtitle" type="java.lang.String" column="Jobtitle"/> 
		   <property name="affiliation" type="java.lang.String" column="Affiliation"/> 
		    <property name="countryid" type="java.lang.String" column="Country_ID"/> 
          <property name="address" type="java.lang.String" column="Address"/> 
		  <property name="building" type="java.lang.String" column="Building"/> 
		  <property name="city" type="java.lang.String" column="City"/> 
		  <property name="state" type="java.lang.String" column="State"/> 
		  <property name="zipcode" type="java.lang.String" column="Zip_Code"/> 
         <property name="note" type="java.lang.String"  column="Note"/>
    </class>
</hibernate-mapping>